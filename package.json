    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "html-pdf": "^3.0.1",
    "html-pdf-node": "^1.0.8",
    "http-errors": "^2.0.0",
    "http-status": "^1.7.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.43",
    "oracledb": "^6.1.0",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
