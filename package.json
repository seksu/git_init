{
  "name": "api-sms",
  "version": "1.0.7",
  "description": "TRIM-EOSL Web-SMS API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint ./src/**/*.ts",
    "db:migrate": "sequelize-cli db:migrate",
    "pretest": "npm run posttest && cross-env NODE_ENV=test npm run db:migrate",
    "test": "jest --coverage --runInBand ./tests/integration",
    "posttest": "rimraf ./database.sqlite"
  },
  "dependencies": {
    "@common/be-core": "^2.0.23",
    "@common/data-file-utils": "^2.0.64",
