    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.5.9",
    "@types/oracledb": "^5.3.1",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "sequelize-cli": "^6.6.1",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
